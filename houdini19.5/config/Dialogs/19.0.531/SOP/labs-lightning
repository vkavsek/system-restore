{
    HSTRETCH
    { WIDTH(0.18) }

    { WIDTH(1.5) }
    { WIDTH(0.08) }

    PARM_BUTTON("exportkind", "Export in ASCII Format") toggle VALUE(exportkind.val);

    { HSTRETCH }

    { WIDTH(0.1) }
}

